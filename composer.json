{
    "name" : "cvar1984/templates",
    "description": "templates",
    "type" : "application",
    "minimum-stability": "stable",
    "license": "MIT",
    "authors": [
        {
            "name": "Cvar1984",
            "email": "gedzsarjuncomuniti@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.4.8"
    },
    "suggest": {
        "pimple": "Depency Injection",
        "fzaninotto/faker": "Random data generator",
        "amphp/parallel": "Parallel processing component",
        "nette/mail": "Handy email transfer libraries",
        "campo/random-user-agent": "Generate real random user-agents.",
        "mikey179/vfsstream": "Virtual file system to mock the real file system in unit tests",
        "psr/log": "Common interface for logging libraries",
        "tracy/tracy": "Addictive friendly design PHP debugger",
        "symfony/console": "Symfony Console Component",
        "ext-Zend-OPcache": "Opcode cache",
        "ext-PDO": "Database support",
        "ext-Phar": "Single PHP archive binaries for deploying console apps",
        "ext-xdebug": "PHP debugger extension"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4",
        "kherge/box": "~2.5",
        "squizlabs/php_codesniffer": "3.*",
        "devster/ubench": "~2.0.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "Cvar1984\\": "src/"
        }
    },
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    }
}
